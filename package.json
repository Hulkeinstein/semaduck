{
  "name": "semaduck",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3003 --webpack",
    "build": "next build",
    "start": "next start -p 3003",
    "lint": "eslint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsc-files --noEmit",
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.91.0",
    "framer-motion": "^12.29.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.4",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "husky": "^9",
    "lint-staged": "^16",
    "playwright": "^1.58.0",
    "prettier": "^3",
    "tsc-files": "^1",
    "typescript": "^5"
  }
}
